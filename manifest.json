{
    "manifest_version": 3,
    "name": "Continue Without Supporting",
    "version": "1.0.0",
    "description": "Automatically select 'continue without supporting' on popups about ad blockers.",
    "content_scripts": [
        {
            "js": ["scripts/content.js"],
            "matches": [
                "*://*/*"
            ]
        }
    ],
    "icons": {
        "16": "assets/16.png",
        "48": "assets/48.png",
        "128": "assets/128.png"
    }
}
